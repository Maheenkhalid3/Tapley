{
  "expo": {
    "name": "Tapley",
    "slug": "tapley",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/woman.png",
    "userInterfaceStyle": "light",
    "scheme": "tapley",
    "backgroundColor": "#ffffff",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foreground.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.yourcompany.tapley",
      "permissions": []  
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.tapley",
      "infoPlist": {} 
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": []  
  }
}